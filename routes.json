{
  "orgs": {
    "listCredentialAuthorizations": {
      "method": "GET",
      "params": {
        "org": {
          "required": true,
          "type": "string"
        }
      },
      "url": "/orgs/:org/credential-authorizations"
    },
    "removeCredentialAuthorization": {
      "method": "DELETE",
      "params": {
        "credential_id": {
          "required": true,
          "type": "integer"
        },
        "org": {
          "required": true,
          "type": "string"
        }
      },
      "url": "/orgs/:org/credential-authorizations/:credential_id"
    }
  },
  "scim": {
    "getProvisioningDetailsForUser": {
      "method": "GET",
      "params": {
        "external_identity_guid": {
          "alias": "scim_user_id",
          "deprecated": true,
          "type": "integer"
        },
        "org": {
          "required": true,
          "type": "string"
        },
        "scim_user_id": {
          "required": true,
