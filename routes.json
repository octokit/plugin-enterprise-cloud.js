  "teams": {
    "createOrUpdateIdPGroupConnections": {
      "method": "PATCH",
      "params": {
        "groups": {
          "required": true,
          "type": "object[]"
        },
        "groups[].group_description": {
          "required": true,
          "type": "string"
        },
        "groups[].group_id": {
          "required": true,
          "type": "string"
        },
        "groups[].group_name": {
