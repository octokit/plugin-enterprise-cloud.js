      "method": "GET",
      "params": {
        "org": {
          "required": true,
          "type": "string"
        },
        "page": {
          "type": "integer"
        },
        "per_page": {
          "type": "integer"
        }
      },
      "url": "/orgs/:org/team-sync/groups"
    }
