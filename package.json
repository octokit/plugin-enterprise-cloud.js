{
  "name": "@octokit/plugin-enterprise-cloud",
  "version": "0.0.0-development",
  "publishConfig": {
    "access": "public"
  },
  "description": "Octokit plugin for GitHubâ€™s Enterprise Cloud REST APIs",
  "scripts": {
    "build": "pika build",
    "lint": "prettier --check '{src,test,scripts}/*' README.md package.json",
    "lint:fix": "prettier --write '{src,test,scripts}/*' README.md package.json",
    "pretest": "npm run -s lint",
    "test": "jest --coverage",
    "update-endpoints": "npm-run-all update-endpoints:*",
    "update-endpoints:fetch-json": "node scripts/update-endpoints/fetch-json",
    "update-endpoints:code": "node scripts/update-endpoints/code"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/octokit/plugin-enterprise-cloud.js.git"
  },
  "keywords": [
    "octokit",
    "github",
    "api",
    "rest",
    "plugin",
    "enterprise"
  ],
  "author": "Gregor Martynus (https://github.com/gr2m)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/octokit/plugin-enterprise-cloud.js/issues"
  },
  "homepage": "https://github.com/octokit/plugin-enterprise-cloud.js#readme",
  "dependencies": {
    "@octokit/types": "^2.0.2"
  },
  "devDependencies": {
    "@octokit/core": "^2.2.0",
    "@octokit/graphql": "^4.2.0",
    "@pika/pack": "^0.5.0",
    "@pika/plugin-build-node": "^0.9.0",
    "@pika/plugin-build-web": "^0.9.0",
    "@pika/plugin-ts-standard-pkg": "^0.9.0",
    "@types/fetch-mock": "^7.3.2",
    "@types/jest": "^25.1.0",
    "@types/node": "^14.0.4",
    "fetch-mock": "^9.0.0",
    "jest": "^24.9.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.1",
    "semantic-release": "^17.0.0",
    "semantic-release-plugin-update-version-in-files": "^1.0.0",
    "sort-keys": "^4.0.0",
    "ts-jest": "^25.1.0",
    "typescript": "^3.7.4"
  },
  "jest": {
    "preset": "ts-jest",
    "coverageThreshold": {
      "global": {
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
      }
    }
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-ts-standard-pkg"
      ],
      [
        "@pika/plugin-build-node"
      ],
      [
        "@pika/plugin-build-web"
      ]
    ]
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github",
      [
        "@semantic-release/npm",
        {
          "pkgRoot": "./pkg"
        }
      ],
      [
        "semantic-release-plugin-update-version-in-files",
        {
          "files": [
            "pkg/dist-web/*",
            "pkg/dist-node/*",
            "pkg/*/version.*"
          ]
        }
      ]
    ]
  }
}
